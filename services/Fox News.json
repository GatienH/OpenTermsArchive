{
  "name": "Fox News",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/foxnews.com.xml",
  "documents": {
    "Closed Captioning Policy": {
      "fetch": "https://www.foxnews.com/closed-captioning",
      "select": "div.content",
      "remove": "img:first-of-type"
    },
    "Privacy Policy": {
      "fetch": "https://www.foxnews.com/privacy-policy",
      "select": ".privacy-policy"
    },
    "Terms of Service": {
      "fetch": "https://www.foxnews.com/terms-of-use",
      "select": ".terms-of-use"
    }
  }
}
