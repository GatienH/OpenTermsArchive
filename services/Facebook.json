{
  "name": "Facebook",
  "documents": {
    "tos": {
      "location": "https://www.facebook.com/legal/terms/plain_text_terms",
      "contentSelector": ".fb_content",
      "filters": [
        "cleanUrls"
      ]
    },
    "privacy": {
      "location": "https://www.facebook.com/about/privacy",
      "contentSelector": "._5tkp",
      "filters": [
        "removeReturnToTopButtons",
        "cleanUrls"
      ]
    },
    "devPolicy": {
      "location": "https://developers.facebook.com/policy",
      "contentSelector": "._4bl9",
      "filters": [
        "removeHelpButtons",
        "cleanUrls"
      ]
    },
    "cookiesPolicy": {
      "location": "https://www.facebook.com/policies/cookies",
      "contentSelector": "._5tkp",
      "filters": [
        "removeImgFromCookiesPolicy",
        "removeReturnToTopButtons",
        "cleanUrls"
      ]
    },
    "commercialTerms": {
      "location": "https://www.facebook.com/legal/commercial_terms",
      "contentSelector": "#content",
      "filters": [
        "removeHelpButtons",
        "cleanUrls"
      ]
    }
  }
