{
  "name": "Facebook Payments",
  "documents": {
    "Terms of service": {
      "fetch": "https://www.facebook.com/payments_terms",
      "select": "#content",
      "filter": [
        "removeHelpButtons",
        "cleanUrls"
      ]
    },
    "Developer Policy": {
      "fetch": "https://developers.facebook.com/policy/credits",
      "select": "._4bl7, ._4bl9",
      "filter": [
        "cleanUrls",
        "numberListCorrectly",
        "correctTitleIndentation"
      ]
    }
  }
}
