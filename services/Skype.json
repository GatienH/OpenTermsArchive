{
    "name": "Skype",
    "documents": {
        "tos": {
            "location": "https://www.skype.com/en/legal/ios/tos/",
            "contentSelector": "div.row:nth-child(2) > div:nth-child(1)"
        },
        "privacy": {
            "location": "https://privacy.microsoft.com/en-gb/privacystatement",
            "contentSelector": "h1, #psp_last_updated, span.Description:nth-child(1), .div_content",
            "filters": [
                "removeIrrelevantModulesFromPrivacyPolicy",
                "removeNavigationHeaders",
                "removeModuleIDs",
                "removeInvisibleText"
            ]
        },
        "tosParent": {
            "location": "https://www.microsoft.com/en-gb/servicesagreement/",
            "contentSelector": ".pageIntro, .div_content",
            "filters": [
            	"removeIrrelevantModulesFromParentTos",
                "removeNavigationHeaders",
                "removeModuleIDs",
                "removeInvisibleText"
            ]
        }
    }
}
